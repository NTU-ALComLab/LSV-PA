.model simple_aig_fig2
.inputs x1 x2 x3
.outputs y4

# Node 5: inputs from x1 (solid) and x2 (solid).
# n5 = x1 & x2
.names x1 x2 n5
11 1


# Node 6: fanins from x2 (solid) and x3 (dotted)  -> n6 = x2 & !x3
.names x2 x3 n6
01 1

# Node 7: fanins from n4 (dotted) and n5 (dotted) -> n7 = !n5 & !n6
.names n5 n6 n7
00 1

# Node 8: fanins from x1 (solid) and n6 (solid) -> n8 = x1 & n7
.names x1 n7 n8
11 1

# Primary outputs (top triangles)
# 4 <- n8
.names n8 y4
1 1

.end