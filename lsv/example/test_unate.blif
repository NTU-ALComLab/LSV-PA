.model unate_test
.inputs a b
.outputs f_ind f_pos f_neg f_bin

# f_ind = b（跟 a 無關）→ 對 a 應該是 independent
.names b f_ind
1 1

# f_pos = a & b → 對 a 是 positive unate
.names a b f_pos
11 1

# f_neg = !a & b → 對 a 是 negative unate
.names a b f_neg
01 1

# f_bin = a XOR b → 對 a 是 binate
.names a b f_bin
01 1
10 1

.end
