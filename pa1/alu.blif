.model alu
.inputs a1 a0 b1 b0 c
.outputs y2 y1 y0

# B' = B XOR c (bitwise). If c=1, invert B; if c=0, keep B.
.names b0 c bx0
01 1
10 1
.names b1 c bx1
01 1
10 1

# y0 = a0 XOR bx0 XOR c
.names a0 bx0 t0
01 1
10 1
.names t0 c y0
01 1
10 1

# c1 = majority(a0, bx0, c)
.names a0 bx0 c c1
110 1
101 1
011 1
111 1

# y1 = a1 XOR bx1 XOR c1
.names a1 bx1 t1
01 1
10 1
.names t1 c1 y1
01 1
10 1

# y2 = majority(a1, bx1, c1)
.names a1 bx1 c1 y2
110 1
101 1
011 1
111 1

.end
